public class Edges {
    public Node node1;
    public Node node2;
    public int cost;


    public Edges() {
    }

    public Edges(Node node1, Node node2, int cost) {
        this.node1 = node1;
        this.node2 = node2;
        this.cost = cost;
    }

    public Node getNode1() {
        return node1;
    }

    public void setNode1(Node node1) {
        this.node1 = node1;
    }

    public Node getNode2() {
        return node2;
    }

    public void setNode2(Node node2) {
        this.node2 = node2;
    }

    public int getCost() {
        return cost;
    }

    public void setCost(int cost) {
        this.cost = cost;
    }

    @Override
    public java.lang.String toString() {
        return "Edges{" + "node1=" + node1 + ", node2=" + node2 + ", cost=" + cost + '}';
    }
}